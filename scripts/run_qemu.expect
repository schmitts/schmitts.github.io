#!/usr/bin/expect -f
set timeout -1
exp_internal 1

log_file qemu.log

spawn qemu-system-riscv64 -machine virt -bios none -kernel kernel/kernel -m 128M -smp 3 -nographic -global virtio-mmio.force-legacy=false -drive file=fs.img,if=none,format=raw,id=x0 -device virtio-blk-device,drive=x0,bus=virtio-mmio-bus.0 -serial mon:stdio

expect "$ "

send "ls\n"

expect "$ "

send "\01x"
expect eof
