#!/usr/bin/expect -f
set send_slow {1 0.01}

set timeout -1

exp_internal 1

log_file qemu.log

spawn -nottycopy qemu-system-riscv64\
    -machine virt\
    -bios none\
    -kernel kernel/kernel\
    -m 128M\
    -smp 3\
    -nographic\
    -global virtio-mmio.force-legacy=false\
    -drive file=fs.img,if=none,format=raw,id=x0\
    -device virtio-blk-device,drive=x0,bus=virtio-mmio-bus.0

expect "$ "

send -s "ls\r"

expect "$ "

send -s "\01x"
expect eof
